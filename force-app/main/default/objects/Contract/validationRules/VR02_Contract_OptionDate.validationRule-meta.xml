<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VR02_Contract_OptionDate</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
!$User.BypassValidationRule__c ,
Option_Date__c &gt;  EndDate
)</errorConditionFormula>
    <errorDisplayField>Option_Date__c</errorDisplayField>
    <errorMessage>Option Date should be smaller than End Date</errorMessage>
</ValidationRule>
