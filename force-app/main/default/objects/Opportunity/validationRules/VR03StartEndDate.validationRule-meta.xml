<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VR03StartEndDate</fullName>
    <active>true</active>
    <errorConditionFormula>AND( 
!$User.BypassValidationRule__c , 
Start_Date__c &gt;  CloseDate 
)</errorConditionFormula>
    <errorDisplayField>Start_Date__c</errorDisplayField>
    <errorMessage>Start Date should be smaller than End Date</errorMessage>
</ValidationRule>
